    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "ejs": "^2.6.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.0.0",
