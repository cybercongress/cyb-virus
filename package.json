    "@litvintech/cyberjs": "git://github.com/litvintech/cyberjs.git",
    "autoprefixer": "^9.6.0",
    "axios": "^0.19.0",
    "bip39": "^3.0.2",
    "cheerio": "^1.0.0-rc.3",
    "dexie": "^2.0.4",
    "ethers": "^4.0.30",
    "font-awesome": "https://github.com/FortAwesome/Font-Awesome.git",
    "hdkey": "^1.1.1",
    "ip-regex": "^4.1.0",
    "ipfs-http-client": "^32.0.1",
    "ipfs-unixfs": "0.1.16",
